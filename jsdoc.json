{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/better-docs",
    "readme": "README.md"
  },
  "templates": {
    "better-docs": {
        "name": "Documentation"
    }
  }
}